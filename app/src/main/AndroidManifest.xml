<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lastview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lv_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lv_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LastView">
        <activity
            android:name=".Agenda_Act"
            android:exported="false" />
        <activity
            android:name=".Info_Act"
            android:exported="false" />
        <activity
            android:name=".Insumos_Act"
            android:exported="true" />
        <activity
            android:name=".Home_ACT"
            android:exported="true" />
        <activity
            android:name=".Main_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>